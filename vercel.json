{
  "rewrites": [
    { "source": "/", "destination": "/html_demo_panel_slope_over_drains.html" },
    { "source": "/demo", "destination": "/html_demo_panel_slope_over_drains.html" }
    { "source": "/waterflow", "destination": "/SandBase.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://notion.so https://*.notion.so"
        }
      ]
    }
  ]
}
